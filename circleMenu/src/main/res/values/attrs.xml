<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="advDrawerLayout">
        <attr name="adl_customBehavior" format="boolean" />
        <attr name="adl_viewScale" format="float" />
        <attr name="adl_viewScrimColor" format="color" />
        <attr name="adl_viewElevation" format="dimension" />
        <attr name="adl_drawerElevation" format="dimension" />
        <attr name="adl_cardBackgroundColor" format="reference|color" />
    </declare-styleable>
    <declare-styleable name="CircleMenu">
        <attr name="menu" format="reference"/>
        <attr name="circleMenuBackground" format="reference"/>
        <attr name="circleMenuDiameter" format="dimension" />
        <attr name="circleMenuOptionRadius" format="dimension" />
        <attr name="optionDiameter" format="dimension" />
        <attr name="optionBackground" format="reference"/>
    </declare-styleable>
    <declare-styleable name="CircleMenuOption">
        <attr name="drawable" format="reference"/>
    </declare-styleable>
</resources>